<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping package="com.dhccity.data.entity">
	<class name="Dataset" table="DATASET">
		<id name="id"  column="ID">
			<generator class="identity">
			</generator>
		</id>
		<property name="name"  column="NAME"/>
		<property name="title"  column="TITLE"/>
		<property name="description"  column="DESCRIPTION"/>
		<property name="state"  column="STATE"/>
		<property name="isPublic"  column="IS_PUBLIC"/>
		<property name="creator"  column="CREATOR"/>
		<property name="createTime"  column="CREATE_TIME" type="timestamp"/>
		<property name="modifyUser"  column="MODIFY_USER"/>
		<property name="modifyTime"  column="MODIFY_TIME" type="timestamp"/>
		<property name="updateRate"  column="UPDATE_RATE"/>
		<property name="downloadNum"  column="DOWNLOAD_NUM"/>
		<property name="readNum"  column="READ_NUM"/>
		<property name="orgId"  column="ORG_ID"/>
		<property name="groupId"  column="GROUP_ID"/>
		<property name="stars"  column="STARS"/>
		<property name="mark_num"  column="MARK_NUM"/>
		<property name="kind"  column="KIND"/>
	</class>
</hibernate-mapping>
