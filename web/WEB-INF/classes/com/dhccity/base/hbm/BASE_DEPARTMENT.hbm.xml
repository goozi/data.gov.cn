<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<!--BASE_DEPARTMENT-->
<!--<mapping resource="com/kingtop/base/hbm/BASE_DEPARTMENT.hbm.xml"/>-->

<hibernate-mapping package="com.dhccity.base.entity">
	<class name="BaseDepartment" table="BASE_DEPARTMENT">
		<id name="id"  column="ID">
			<generator class="identity">
			</generator>
		</id>
		<property name="name"  column="NAME"/>
		<property name="state"  column="STATE"/>
		<property name="sequ"  column="SEQU"/>
		<property name="parentId"  column="PARENT_ID"/>
		<property name="leaderId"  column="LEADER_ID"/>
		<property name="code"  column="CODE"/>
		<property name="fullName"  column="FULL_NAME"/>
		<property name="mainLevel"  column="MAIN_LEVEL"/>
		<property name="subLevel"  column="SUB_LEVEL"/>
		<property name="property"  column="PROPERTY"/>
		<property name="address"  column="ADDRESS"/>
		<property name="postCode"  column="POST_CODE"/>
		<property name="canton"  column="CANTON"/>
		<property name="tel"  column="TEL"/>
		<property name="fax"  column="FAX"/>
		<property name="explain"  column="EXPLAIN"/>
		<property name="meno"  column="MENO"/>
	</class>
</hibernate-mapping>
