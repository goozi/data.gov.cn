<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping package="com.dhccity.app.entity">
	<class name="App" table="app">
		<id name="id"  column="ID">
			<generator class="identity">
			</generator>
		</id>
		<property name="name"  column="name"/>
		<property name="cnName"  column="cn_name"/>
		<property name="description"  column="description"/>
		<property name="url"  column="url"/>
		<property name="developer"  column="developer"/>
		<property name="submitterId"  column="submitter_id"/>
		<property name="submitTime"  column="submit_time" type="timestamp"/>
		<property name="modifyUser"  column="modify_user"/>
		<property name="modifyTime"  column="modify_time" type="timestamp"/>
		<property name="appThumbnail"  column="app_thumbnail"/>
		<property name="appPic"  column="app_pic"/>
		<property name="appCharge"  column="app_charge"/>
		<property name="category"  column="category"/>
		<property name="appOs"  column="app_os"/>
		<property name="markNum"  column="mark_num"/>
		<property name="stars"  column="stars"/>
		<property name="readNum"  column="read_num"/>
	</class>
</hibernate-mapping>
