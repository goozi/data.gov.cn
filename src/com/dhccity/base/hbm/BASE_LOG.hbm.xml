<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<!--BASE_LOG-->
<!--<mapping resource="com/kingtop/base/hbm/BASE_LOG.hbm.xml"/>-->

<hibernate-mapping package="com.dhccity.base.entity">
	<class name="BaseLog" table="BASE_LOG">
		<id name="id"  column="ID">
			<generator class="identity">
			</generator>
		</id>
		<property name="userId"  column="USER_ID"/>
		<property name="content"  column="CONTENT"/>
		<property name="ip"  column="IP"/>
		<property name="nodeTime"  column="NODE_TIME"  type="timestamp"/>
		<property name="pcName"  column="PC_NAME"/>
		<property name="meno"  column="MENO"/>
		<property name="systemName"  column="SYSTEM_NAME"/>
		<property name="moduleName"  column="MODULE_NAME"/>
		<property name="userName"  column="USER_NAME"/>
	</class>
</hibernate-mapping>
