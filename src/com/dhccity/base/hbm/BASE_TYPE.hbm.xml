<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<!--BASE_TYPE-->
<!--<mapping resource="com/kingtop/xmsl/setup/hbm/BASE_TYPE.hbm.xml"/>-->

<hibernate-mapping package="com.dhccity.base.entity">
	<class name="BaseType" table="BASE_TYPE">
		<id name="id"  column="ID">
			<generator class="identity">
			</generator>
		</id>
		<property name="name"  column="NAME"/>
		<property name="code"  column="CODE"/>
		<property name="state"  column="STATE"/>
		<property name="isDefault"  column="IS_DEFAULT"/>
		<property name="sequ"  column="SEQU"/>
		<property name="parentId"  column="PARENT_ID"/>
		<property name="userNames"  column="USER_NAMES"/>
		<property name="userIds"  column="USER_IDS"/>
		<property name="value"  column="VALUE"/>
		<property name="type"  column="TYPE"/>
		<property name="description"  column="DESCRIPTION"/>
		<property name="meno"  column="MENO"/>
        <property name="securityLevel"  column="SECURITY_LEVEL"/>
        <property name="isParent"  column="IS_PARENT"/>
	</class>
</hibernate-mapping>
