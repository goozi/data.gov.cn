<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping package="com.dhccity.data.entity">
	<class name="DataResource" table="DATA_RESOURCE">
		<id name="id"  column="ID">
			<generator class="native">
				<param name="sequence">DATA_RESOURCE_ID_SEQU</param>
			</generator>
		</id>
		<property name="name"  column="NAME"/>
		<property name="title"  column="TITLE"/>
		<property name="description"  column="DESCRIPTION"/>
		<property name="resType"  column="RES_TYPE"/>
		<property name="sourceUrl"  column="SOURCE_URL"/>
		<property name="version"  column="VERSION"/>
		<property name="creator"  column="CREATOR"/>
		<property name="createDate"  column="CREATE_DATE" type="timestamp"/>
		<property name="modifyUser"  column="MODIFY_USER"/>
		<property name="modifyDate"  column="MODIFY_DATE" type="timestamp"/>
		<property name="downloadNum"  column="DOWNLOAD_NUM"/>
		<property name="fileId"  column="FILE_ID"/>
		<property name="datasetId"  column="DATASET_ID"/>
	</class>
</hibernate-mapping>
