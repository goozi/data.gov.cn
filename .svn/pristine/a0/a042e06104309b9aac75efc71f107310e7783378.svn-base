<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping package="com.dhccity.home.entity">
	<class name="Comment" table="user_comment">
		<id name="id"  column="ID">
			<generator class="native">
				<param name="sequence">comment_ID_SEQU</param>
			</generator>
		</id>
		<property name="articleId"  column="article_id"/>
		<property name="content"  column="content"/>
		<property name="state"  column="state"/>
		<property name="ip"  column="IP"/>
		<property name="creator"  column="creator"/>
		<property name="createTime"  column="create_time"/>
		<property name="modifyUser"  column="modify_user"/>
		<property name="modifyTime"  column="modify_time"/>
	</class>
</hibernate-mapping>
