@charset "utf-8";

body {
  background: #fff;
  margin: 0px;
  padding: 0px;
  font-family: arial,"Microsoft Yahei"!important;
}
embed{display:none;}
ul{margin:0; padding:0;}
li{list-style: none;}


.initstyle .tree .treeState,
.initstyle .tree .treeIcon,
.tree .treeState,
.tree .treeIcon{ 
  display: inline-block;
  font: normal normal normal 14px/1 FontAwesome;
  font-size: inherit;
  text-rendering: auto;
  -webkit-font-smoothing: antialiased;
  -moz-osx-font-smoothing: grayscale;
  transform: translate(0, 0);
  font-size: 14px;
  display: inline-block;
  text-align: right;
  }
  
.tree{ overflow: hidden;  border-left:0;border:0; border-right: 1px solid #fff; }
.tree .tree-Li{ overflow:hidden; cursor:pointer; vertical-align: bottom;line-height:36px; padding:0;  border-top:1px solid #fff;  }
.tree .tree-Div{overflow:hidden; position: relative; border-bottom:1px solid #e0e0e0;}
.tree .tree-Div .fa,
.tree .tree-Div .treeIcon   {margin:10px 10px 0 0;float:left; }
.tree .tree-Div .treeState{ position: absolute; right:10px; top:10px;  }
.tree .tree-Div .tree-Desc a,
.tree .tree-Div .tree-Desc{ font-size:14px; color:#666; display:block;}
.tree .tree-Div .tree-Desc{padding:0 0px;}

.tree .tree-Div .treeIcon:before {content: "\f016";}
.tree .tree-Div.withSub .treeIcon:before   { content: "\f07b";}
.tree .tree-Div.withSub.open   .treeIcon:before,
.tree .tree-Div.withSub.active .treeIcon:before   { content: "\f07c";}

.tree .tree-Div .treeState:before {content: "\f10c"; color:#ccc;}
.tree .tree-Div1 .treeState:before{content: "\f10c";}
.tree .tree-Div.withSub .treeState:before{ content: "\f054";}
.tree .tree-Div.withSub.open   .treeState:before,
.tree .tree-Div.withSub.active .treeState:before{content: "\f078";}

.tree .tree-Div{background-color:#f9f9f9;}
.tree .tree-Div1{background-color:#f3f3f3;}
.tree .tree-Div.hover,
.tree .tree-Div.active{background-color:#fffff7;}
.tree .tree-Div1.hover,
.tree .tree-Div1.active{background-color:#fffccb;}
.tree .tree-Div2.hover,
.tree .tree-Div2.active{background-color:#fffeed;}
.tree .tree-Div.active .tree-Desc,
.tree .tree-Div.active .tree-Desc a,
.tree .tree-Div.active .treeIcon:before,
.tree .tree-Div.active .treeState:before{color:#3382cd;font-weight:bold;}
.tree .tree-Div1.active .tree-Desc{ }
.tree.noIcon .iconTree{display:none;}



