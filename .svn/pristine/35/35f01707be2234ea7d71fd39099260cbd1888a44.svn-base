<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<!--BASE_MESSAGE-->
<!--<mapping resource="com/kingtop/xmsl/common/hbm/BASE_MESSAGE.hbm.xml"/>-->

<hibernate-mapping package="com.dhccity.base.entity">
	<class name="BaseMessage" table="BASE_MESSAGE">
		<id name="id"  column="ID">
			<generator class="identity">
			</generator>
		</id>
		<property name="code"  column="CODE"/>
		<property name="content"  column="CONTENT"/>
		<property name="explain"  column="EXPLAIN"/>
	</class>
</hibernate-mapping>
