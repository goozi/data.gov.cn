<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE web-app PUBLIC "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
        "http://java.sun.com/dtd/web-app_2_3.dtd">
<web-app>
    <display-name>/</display-name>
    <context-param>
        <param-name>contextConfigLocation</param-name>
        <param-value>/WEB-INF/classes/spring.xml</param-value>
    </context-param>
    <filter>
        <filter-name>SecurityFilter</filter-name>
        <filter-class>org.light.system.HttpFilter</filter-class>
        <init-param>
            <param-name>security</param-name>
            <param-value>true</param-value>
        </init-param>
    </filter>
    <filter>
        <filter-name>Filter</filter-name>
        <filter-class>org.light.system.HttpFilter</filter-class>
        <init-param>
            <param-name>security</param-name>
            <param-value>false</param-value>
        </init-param>
    </filter>
    <filter-mapping>
        <filter-name>SecurityFilter</filter-name>
        <url-pattern>*.jsp</url-pattern>
    </filter-mapping>
    <filter-mapping>
        <filter-name>SecurityFilter</filter-name>
        <url-pattern>*.do</url-pattern>
    </filter-mapping>
    <filter-mapping>
        <filter-name>Filter</filter-name>
        <url-pattern>*.htm</url-pattern>
    </filter-mapping>
    <filter-mapping>
        <filter-name>Filter</filter-name>
        <url-pattern>*.jsps</url-pattern>
    </filter-mapping>
    <filter-mapping>
        <filter-name>Filter</filter-name>
        <url-pattern>*.dox</url-pattern>
    </filter-mapping>
    <listener>
        <listener-class>org.light.system.Spring</listener-class>
    </listener>
    <listener>
        <listener-class>org.light.OnlineUser</listener-class>
    </listener>
    <servlet>
        <servlet-name>format</servlet-name>
        <servlet-class>org.light.system.Startup</servlet-class>
        <load-on-startup>0</load-on-startup>
    </servlet>
    <servlet>
        <servlet-name>xmlaction</servlet-name>
        <servlet-class>com.dhccity.base.servlet.XmlAction</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>SecurityAction</servlet-name>
        <servlet-class>com.dhccity.base.servlet.SecurityAction</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>UserSecurityAction</servlet-name>
        <servlet-class>com.dhccity.base.servlet.UserSecurityAction</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>loginaction</servlet-name>
        <servlet-class>com.dhccity.base.servlet.LoginAction</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>TypeAction</servlet-name>
        <servlet-class>com.dhccity.base.servlet.TypeAction</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>MenuAction</servlet-name>
        <servlet-class>com.dhccity.base.servlet.MenuAction</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>DepartmentAction</servlet-name>
        <servlet-class>com.dhccity.base.servlet.DepartmentAction</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>UserAction</servlet-name>
        <servlet-class>com.dhccity.base.servlet.UserAction</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>ParameterAction</servlet-name>
        <servlet-class>com.dhccity.base.servlet.ParameterAction</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>DocumentAction</servlet-name>
        <servlet-class>com.dhccity.base.servlet.DocumentAction</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>LogAction</servlet-name>
        <servlet-class>com.dhccity.base.servlet.LogAction</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>UserParameterAction</servlet-name>
        <servlet-class>com.dhccity.base.servlet.UserParameterAction</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>ArticleAction</servlet-name>
        <servlet-class>com.dhccity.home.servlet.ArticleAction</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>DataResourceAction</servlet-name>
        <servlet-class>com.dhccity.data.servlet.DataResourceAction</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>DatasetAction</servlet-name>
        <servlet-class>com.dhccity.data.servlet.DatasetAction</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>AppAction</servlet-name>
        <servlet-class>com.dhccity.app.servlet.AppAction</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>AppDatasetRelationAction</servlet-name>
        <servlet-class>com.dhccity.app.servlet.AppDatasetRelationAction</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>DataGroupAction</servlet-name>
        <servlet-class>com.dhccity.data.servlet.DataGroupAction</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>OrgAction</servlet-name>
        <servlet-class>com.dhccity.data.servlet.OrgAction</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>CommentAction</servlet-name>
        <servlet-class>com.dhccity.home.servlet.CommentAction</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>systemaction</servlet-name>
        <servlet-class>com.dhccity.base.servlet.SystemAction</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>SecurityAction</servlet-name>
        <url-pattern>/base/securityAction.do</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>UserSecurityAction</servlet-name>
        <url-pattern>/base/userSecurityAction.do</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>loginaction</servlet-name>
        <url-pattern>/loginAction.dox</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>TypeAction</servlet-name>
        <url-pattern>/base/typeAction.do</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>MenuAction</servlet-name>
        <url-pattern>/base/menuAction.do</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>DepartmentAction</servlet-name>
        <url-pattern>/base/departmentAction.do</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>UserAction</servlet-name>
        <url-pattern>/base/userAction.do</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>ParameterAction</servlet-name>
        <url-pattern>/base/parameterAction.do</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>DocumentAction</servlet-name>
        <url-pattern>/base/documentAction.do</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>LogAction</servlet-name>
        <url-pattern>/base/logAction.do</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>UserParameterAction</servlet-name>
        <url-pattern>/base/userParameterAction.do</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>systemaction</servlet-name>
        <url-pattern>/systemAction.do</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>ArticleAction</servlet-name>
        <url-pattern>/home/carouselAction.do</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>DataResourceAction</servlet-name>
        <url-pattern>/data/dataResourceAction.do</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>DatasetAction</servlet-name>
        <url-pattern>/data/datasetAction.do</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>AppAction</servlet-name>
        <url-pattern>/app/appAction.do</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>AppDatasetRelationAction</servlet-name>
        <url-pattern>/app/appRelAction.do</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>DataGroupAction</servlet-name>
        <url-pattern>/data/dataGroupAction.do</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>OrgAction</servlet-name>
        <url-pattern>/data/orgAction.do</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>CommentAction</servlet-name>
        <url-pattern>/home/commentAction.do</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>xmlaction</servlet-name>
        <url-pattern>/base/xmlAction.dox</url-pattern>
    </servlet-mapping>
    <session-config>
        <session-timeout>10</session-timeout>
    </session-config>
    <welcome-file-list>
        <welcome-file>index.html</welcome-file>
        <welcome-file>index.htm</welcome-file>
        <welcome-file>index.jsp</welcome-file>
        <welcome-file>index.jsps</welcome-file>
    </welcome-file-list>
</web-app>
