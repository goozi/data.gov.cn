<%@ page contentType="text/html; charset=GBK"%>
<%@ include file="../include.jsp" %>
<%@ page import="com.dhccity.data.entity.DataResource" %>
<%
	long id=req.getLong("id");
	DataResource dataResource = (DataResource) new DataResource().findById(id);
%>
<html>
<head>
	<meta http-equiv="Content-Type" content="text/html; charset=gb2312">
	<LINK  href="../style/default.css" rel=stylesheet>
	<link rel="stylesheet" href="../htc/default.jss" type="text/css">
	<title><%=Config.SystemName%></title>
</head>
<body>
	<form name="dataForm" method="post" action="dataResourceAction.do?action=updateData" Check="true" Change="true">
	<table class="edit"  cellspacing=1 cellpadding=3 width="95%" align=center border=0>
		<tr class="title">
			<td colspan="4"><b>数据资源编辑</b></td>
		</tr>
		<tr>
			<td width="15%"  align="right">名称</td>
			<td><input name="name" type="text" size="30" value="<%=dataResource.getName()%>" validType="maxLength[160]" class="easyui-validatebox"></td>
			<td width="15%"  align="right">标题</td>
			<td><input name="title" type="text" size="30" value="<%=dataResource.getTitle()%>" validType="maxLength[160]" class="easyui-validatebox"></td>
		</tr>
		<tr>
			<td width="15%"  align="right">描述</td>
			<td><input name="desc" type="text" size="30" value="<%=dataResource.getDesc()%>" validType="maxLength[800]" class="easyui-validatebox"></td>
			<td width="15%"  align="right">文件类型</td>
			<td><input name="fileType" type="text" size="30" DataType="int" value="<%=dataResource.getFileType()%>" validType="maxLength[18]" class="easyui-validatebox"></td>
		</tr>
		<tr>
			<td width="15%"  align="right">来源URL</td>
			<td><input name="sourceUrl" type="text" size="30" value="<%=dataResource.getSourceUrl()%>" validType="maxLength[160]" class="easyui-validatebox"></td>
			<td width="15%"  align="right">版本</td>
			<td><input name="version" type="text" size="30" DataType="float" value="<%=dataResource.getVersion()%>" validType="maxLength[1]" class="easyui-validatebox"></td>
		</tr>
		<tr>
			<td width="15%"  align="right">创建人</td>
			<td><input name="creator" type="text" size="30" DataType="int" value="<%=dataResource.getCreator()%>" validType="maxLength[18]" class="easyui-validatebox"></td>
			<td width="15%"  align="right">创建时间</td>
			<td><input name="createDate" type="text" size="30" DataType="date" value="<%=dataResource.getCreateDate()%>" validType="maxLength[6]" class="easyui-validatebox"></td>
		</tr>
		<tr>
			<td width="15%"  align="right">修改人</td>
			<td><input name="modifyUser" type="text" size="30" DataType="int" value="<%=dataResource.getModifyUser()%>" validType="maxLength[18]" class="easyui-validatebox"></td>
			<td width="15%"  align="right">修改时间</td>
			<td><input name="modifyDate" type="text" size="30" DataType="date" value="<%=dataResource.getModifyDate()%>" validType="maxLength[6]" class="easyui-validatebox"></td>
		</tr>
		<tr>
			<td width="15%"  align="right">下载数</td>
			<td><input name="downloadNum" type="text" size="30" DataType="int" value="<%=dataResource.getDownloadNum()%>" validType="maxLength[8]" class="easyui-validatebox"></td>
			<td width="15%"  align="right">文件ID</td>
			<td><input name="fileId" type="text" size="30" DataType="int" value="<%=dataResource.getFileId()%>" validType="maxLength[18]" class="easyui-validatebox"></td>
		</tr>
		<tr>
			<td width="15%"  align="right">数据集ID</td>
			<td><input name="datasetId" type="text" size="30" DataType="int" value="<%=dataResource.getDatasetId()%>" validType="maxLength[18]" class="easyui-validatebox"></td>
		<td></td><td></td></tr>
	</table>
	<table width="95%" border="0" align="center" cellpadding="0" cellspacing="0">
		<tr>
			<td height="29" align="center">
				<input name="id" type="hidden" value="<%=dataResource.getId()%>">
				<input type="submit" class="btn" border="0" name="sure" value="确定" >
				<input type="button" class="btn" border="0" name="return" value="返回" onclick="history.back(-1);">
			</td>
		</tr>
	</table>
	</form>
</body>
</html>
