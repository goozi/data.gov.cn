<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping package="com.dhccity.home.entity">
	<class name="Article" table="article">
		<id name="id"  column="ID">
			<generator class="native">
				<param name="sequence">ARTICLE_ID_SEQU</param>
			</generator>
		</id>
		<property name="title"  column="title"/>
		<property name="content"  column="content"/>
		<property name="type"  column="type"/>
		<property name="creator"  column="creator"/>
		<property name="state"  column="state"/>
		<property name="isPublic"  column="is_public"/>
		<property name="isCommentable"  column="is_commentable"/>
		<property name="isTop"  column="is_top"/>
		<property name="createTime"  column="create_time"/>
		<property name="modifyUser"  column="modify_user"/>
		<property name="modifyTime"  column="modify_time"/>
		<property name="readNum"  column="read_num"/>
		<property name="ip"  column="IP"/>
		<property name="commentNum"  column="COMMENT_NUM"/>
		<property name="forumId"  column="FORUM_ID"/>
		<property name="relatId"  column="RELAT_ID"/>
	</class>
</hibernate-mapping>
