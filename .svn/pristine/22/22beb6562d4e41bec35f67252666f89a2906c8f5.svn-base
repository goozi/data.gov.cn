.btn {
	Height: 25px;
	BORDER-RIGHT: #31557f 1px solid;
	PADDING-RIGHT: 4px;
	BORDER-TOP: #cce3ff 1px solid;
	PADDING-LEFT: 4px;
	FONT-WEIGHT: normal;
	FONT-SIZE: 12px;
	BACKGROUND: #7688b1;
	PADDING-BOTTOM: 2px;
	MARGIN: 2px;
	BEHAVIOR: url("../css/changeButton.htc");
	WORD-SPACING: 0px;
	VERTICAL-ALIGN: middle;
	BORDER-LEFT: #cce3ff 1px solid;
	CURSOR: hand;
	COLOR: #ffffff;
	PADDING-TOP: 2px;
	BORDER-BOTTOM: #31557f 1px solid;
	LETTER-SPACING: 0px;
	HEIGHT: 20px
}

