	cellpadding: 0
}

.query_table tr {
	FONT-WEIGHT: normal;
	FONT-SIZE: 12px;
	COLOR: #003333;
	FONT-STYLE: normal;
	BACKGROUND-COLOR: #efefef;
	align: center
}

.query_table input {
	BORDER-RIGHT: black 1px solid;
	BORDER-TOP: black 1px solid;
	FONT-SIZE: 12px;
	IME-MODE: active;
	BORDER-LEFT: black 1px solid;
	COLOR: #000000;
	BORDER-BOTTOM: black 1px solid;
	FONT-FAMILY: "Times New Roman", "SimSun";
	HEIGHT: 18px
}

