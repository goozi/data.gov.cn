<%@ page contentType="text/html; charset=GBK"%>
<%@ include file="../include.jsp" %>
<%@ page import="com.dhccity.data.entity.DataResource" %>
<%
%>
<html xmlns:kingtop>
<head>
	<meta http-equiv="Content-Type" content="text/html; charset=gb2312">
	<LINK  href="../style/default.css" rel=stylesheet>
	<link rel="stylesheet" href="/kindeditor/themes/default/default.css" />
<link rel="stylesheet" href="/kindeditor/plugins/code/prettify.css" />
<script type="text/javascript" src="../js/jquery-1.11.3.js"></script>
<script type="text/javascript" src="../js/jquery.validate.js"></script>
<script charset="utf-8" src="/kindeditor/kindeditor.js"></script>
<script charset="utf-8" src="/kindeditor/lang/zh_CN.js"></script>
<script charset="utf-8" src="/kindeditor/plugins/code/prettify.js"></script>
	<title><%=Config.SystemName%></title>
</head>
<body>
	<form name="dataForm" method="post" ENCTYPE="multipart/form-data" action="dataResourceAction.do?action=addData" Check="true" Change="true">
	<table class="edit"  cellspacing=1 cellpadding=3 width="95%" align=center border=0>
		<tr class="title">
			<td colspan="4"><b>数据资源新增</b></td>
		</tr>
		<tr>
			<td width="15%"  align="right">名称</td>
			<td><input name="name" type="text" size="30" maxByte="160"></td>
			<td width="15%"  align="right">标题</td>
			<td><input name="title" type="text" size="30" maxByte="160"></td>
		</tr>
		<tr>
			<td width="15%"  align="right">描述</td>
			<td><textarea id="editor_id" name="desc"
			style="width: 700px; height: 300px;">
</textarea></td>
			
		</tr>
		<tr>
			<td width="15%"  align="right">来源URL</td>
			<td><input name="sourceUrl" type="text" size="30" maxByte="160"></td>
			<td width="15%"  align="right">文件ID</td>
			<td><input name="fileId" type="text" size="30" DataType="int" validType="maxLength[18]" class="easyui-validatebox"></td>
		</tr>
		<tr>
			<td width="15%"  align="right">版本</td>
			<td><input name="version" type="text" size="30" DataType="float" maxByte="1"></td>
		</tr>
	</table>
	<table width="95%" border="0" align="center" cellpadding="0" cellspacing="0">
		<tr>
			<td height="29" align="center">
				
				<input type="submit" class="btn" border="0" name="sure" value="确定" >
				<input type="button" class="btn" border="0" name="return" value="返回" onclick="history.back(-1);">
			</td>
		</tr>
	</table>
	</form>
</body>
</html>
