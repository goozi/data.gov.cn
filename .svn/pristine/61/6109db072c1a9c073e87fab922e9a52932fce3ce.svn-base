<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<!--BASE_SECURITY-->
<!--<mapping resource="com/kingtop/base/hbm/BASE_SECURITY.hbm.xml"/>-->

<hibernate-mapping package="com.dhccity.base.entity">
	<class name="BaseSecurity" table="BASE_SECURITY">
		<id name="id"  column="ID">
			<generator class="identity">
			</generator>
		</id>
		<property name="name"  column="NAME"/>
		<property name="state"  column="STATE"/>
		<property name="sequ"  column="SEQU"/>
		<property name="parentId"  column="PARENT_ID"/>
		<property name="value"  column="VALUE"/>
		<property name="type"  column="TYPE"/>
		<property name="description"  column="DESCRIPTION"/>
		<property name="meno"  column="MENO"/>
        <property name="securityLevel"  column="SECURITY_LEVEL"/>
	</class>
</hibernate-mapping>
