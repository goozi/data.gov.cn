<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<!--BASE_DOCUMENT_PATH-->
<!--<mapping resource="com/kingtop/xmsl/common/hbm/BASE_DOCUMENT_PATH.hbm.xml"/>-->

<hibernate-mapping package="com.dhccity.base.entity">
	<class name="BaseDocumentPath" table="BASE_DOCUMENT_PATH">
		<id name="id"  column="ID">
			<generator class="native">
				<param name="sequence">BASE_DOCUMENT_PATH_ID_SEQU</param>
			</generator>
		</id>
		<property name="sourceCode"  column="SOURCE_CODE"/>
		<property name="path"  column="PATH"/>
		<property name="explain"  column="EXPLAIN"/>
	</class>
</hibernate-mapping>
