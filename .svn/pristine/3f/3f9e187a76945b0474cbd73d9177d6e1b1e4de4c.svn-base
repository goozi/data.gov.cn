<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<!--BASE_USER_GROUP-->
<!--<mapping resource="com/kingtop/base/hbm/BASE_USER_GROUP.hbm.xml"/>-->

<hibernate-mapping package="com.dhccity.base.entity">
	<class name="BaseUserGroup" table="BASE_USER_GROUP">
		<id name="id"  column="ID">
			<generator class="identity">
			</generator>
		</id>
		<property name="userId"  column="USER_ID"/>
		<property name="departmentId"  column="DEPARTMENT_ID"/>
		<property name="groupId"  column="GROUP_ID"/>
		<property name="type"  column="TYPE"/>
		<property name="sequ"  column="SEQU"/>
	</class>
</hibernate-mapping>
