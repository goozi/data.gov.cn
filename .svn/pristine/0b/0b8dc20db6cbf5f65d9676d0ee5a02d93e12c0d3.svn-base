<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<!--BASE_DOCUMENT_TYPE-->
<!--<mapping resource="com/kingtop/xmsl/common/hbm/BASE_DOCUMENT_TYPE.hbm.xml"/>-->

<hibernate-mapping package="com.dhccity.base.entity">
	<class name="BaseDocumentType" table="BASE_DOCUMENT_TYPE">
		<id name="id"  column="ID">
			<generator class="identity">
			</generator>
		</id>
		<property name="postfix"  column="POSTFIX"/>
		<property name="icon"  column="ICON"/>
		<property name="type"  column="TYPE"/>
        <property name="mime"  column="MIME"/>
	</class>
</hibernate-mapping>
