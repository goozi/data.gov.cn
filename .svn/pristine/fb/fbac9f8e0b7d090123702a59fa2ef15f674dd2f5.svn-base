<%@ page contentType="text/html; charset=GBK"%>
<%@ include file="../include.jsp" %>
<%@ page import="com.dhccity.data.entity.Dataset" %>
<%
	long id=req.getLong("id");
	Dataset dataset = (Dataset) new Dataset().findById(id);
%>
<html>
<head>
	<meta http-equiv="Content-Type" content="text/html; charset=gb2312">
	<LINK  href="../style/default.css" rel=stylesheet>
	<link rel="stylesheet" href="../htc/default.jss" type="text/css">
	<title><%=Config.SystemName%></title>
</head>
<body>
	<form name="dataForm" method="post" action="datasetAction.do?action=updateData" Check="true" Change="true">
	<table class="edit"  cellspacing=1 cellpadding=3 width="95%" align=center border=0>
		<tr class="title">
			<td colspan="4"><b>数据集编辑</b></td>
		</tr>
		<tr>
			<td width="15%"  align="right">名称</td>
			<td><input name="name" type="text" size="30" value="<%=dataset.getName()%>" validType="maxLength[160]" class="easyui-validatebox"></td>
			<td width="15%"  align="right">标题</td>
			<td><input name="title" type="text" size="30" value="<%=dataset.getTitle()%>" validType="maxLength[160]" class="easyui-validatebox"></td>
		</tr>
		<tr>
			<td width="15%"  align="right">描述</td>
			<td><input name="description" type="text" size="30" value="<%=dataset.getDescription()%>" maxByte="2000"></td>
			<td width="15%"  align="right">状态</td>
			<td><input name="state" type="text" size="30" DataType="int" value="<%=dataset.getState()%>" validType="maxLength[1]" class="easyui-validatebox"></td>
		</tr>
		<tr>
			<td width="15%"  align="right">是否公开</td>
			<td><input name="isPublic" type="text" size="30" DataType="int" value="<%=dataset.getIsPublic()%>" validType="maxLength[1]" class="easyui-validatebox"></td>
			<td width="15%"  align="right">创建人</td>
			<td><input name="creator" type="text" size="30" DataType="int" value="<%=dataset.getCreator()%>" validType="maxLength[18]" class="easyui-validatebox"></td>
		</tr>
		<tr>
			<td width="15%"  align="right">创建时间</td>
			<td><input name="createTime" type="text" size="30" DataType="date" value="<%=dataset.getCreateTime()%>" validType="maxLength[6]" class="easyui-validatebox"></td>
			<td width="15%"  align="right">修改人</td>
			<td><input name="modifyUser" type="text" size="30" DataType="int" value="<%=dataset.getModifyUser()%>" validType="maxLength[18]" class="easyui-validatebox"></td>
		</tr>
		<tr>
			<td width="15%"  align="right">修改时间</td>
			<td><input name="modifyTime" type="text" size="30" DataType="date" value="<%=dataset.getModifyTime()%>" validType="maxLength[6]" class="easyui-validatebox"></td>
			<td width="15%"  align="right">更新频率</td>
			<td><input name="updateRate" type="text" size="30" value="<%=dataset.getUpdateRate()%>" validType="maxLength[64]" class="easyui-validatebox"></td>
		</tr>
		<tr>
			<td width="15%"  align="right">下载数</td>
			<td><input name="downloadNum" type="text" size="30" DataType="int" value="<%=dataset.getDownloadNum()%>" validType="maxLength[8]" class="easyui-validatebox"></td>
			<td width="15%"  align="right">查看数</td>
			<td><input name="readNum" type="text" size="30" DataType="int" value="<%=dataset.getReadNum()%>" validType="maxLength[8]" class="easyui-validatebox"></td>
		</tr>
		<tr>
			<td width="15%"  align="right">所属机构</td>
			<td><input name="orgId" type="text" size="30" DataType="int" value="<%=dataset.getOrgId()%>" validType="maxLength[18]" class="easyui-validatebox"></td>
			<td width="15%"  align="right">所属专题</td>
			<td><input name="groupId" type="text" size="30" DataType="int" value="<%=dataset.getGroupId()%>" validType="maxLength[18]" class="easyui-validatebox"></td>
		</tr>
	</table>
	<table width="95%" border="0" align="center" cellpadding="0" cellspacing="0">
		<tr>
			<td height="29" align="center">
				<input name="id" type="hidden" value="<%=dataset.getId()%>">
				<input type="submit" class="btn" border="0" name="sure" value="确定" >
				<input type="button" class="btn" border="0" name="return" value="返回" onclick="history.back(-1);">
			</td>
		</tr>
	</table>
	</form>
</body>
</html>
